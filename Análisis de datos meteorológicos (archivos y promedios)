#include <bits/stdc++.h>
using namespace std;

int main(){
    ofstream out("datos.txt");
    out<<"25.1 26.3 24.8 27.2 28.0\n";
    out.close();

    ifstream in("datos.txt");
    vector<double> temps; double t;
    while(in>>t) temps.push_back(t);
    in.close();

    double prom=accumulate(temps.begin(),temps.end(),0.0)/temps.size();
    cout<<"Temperatura promedio: "<<prom<<" Â°C\n";
    return 0;
}
