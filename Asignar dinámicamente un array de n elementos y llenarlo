#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Ingrese tamaÃ±o del array: ";
    cin >> n;

    int* arr = new int[n];

    for (int i = 0; i < n; i++) {
        cout << "Elemento [" << i << "]: ";
        cin >> arr[i];
    }

    cout << "Array ingresado: ";
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }

    delete[] arr; // liberar memoria
    return 0;
}
