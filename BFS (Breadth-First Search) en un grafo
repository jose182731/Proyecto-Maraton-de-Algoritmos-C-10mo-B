#include <iostream>
#include <vector>
#include <queue>
using namespace std;

void bfs(int start, vector<vector<int>>& adj){
    vector<bool> visitado(adj.size(), false);
    queue<int> q;
    q.push(start);
    visitado[start] = true;

    while(!q.empty()){
        int u = q.front(); q.pop();
        cout << u << " ";
        for(int v: adj[u]){
            if(!visitado[v]){
                visitado[v] = true;
                q.push(v);
            }
        }
    }
}

int main(){
    int n = 4;
    vector<vector<int>> adj(n);
    adj[0] = {1,2};
    adj[1] = {0,2};
    adj[2] = {0,1,3};
    adj[3] = {2};
    bfs(0, adj);
    return 0;
}
