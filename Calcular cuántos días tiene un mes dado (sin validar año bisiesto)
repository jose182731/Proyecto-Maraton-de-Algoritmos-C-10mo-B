#include <iostream>
using namespace std;

int main() {
    int mes;
    cout << "Ingrese el número del mes (1-12): ";
    cin >> mes;

    switch (mes) {
        case 4: case 6: case 9: case 11:
            cout << "Tiene 30 días." << endl;
            break;
        case 2:
            cout << "Tiene 28 días." << endl;
            break;
        case 1: case 3: case 5: case 7: case 8: case 10: case 12:
            cout << "Tiene 31 días." << endl;
            break;
        default:
            cout << "Mes inválido." << endl;
    }

    return 0;
}
