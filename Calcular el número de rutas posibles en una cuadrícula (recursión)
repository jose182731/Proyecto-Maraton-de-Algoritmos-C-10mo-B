#include <iostream>
using namespace std;

int rutas(int filas,int columnas){
    if(filas==1 || columnas==1) return 1;
    return rutas(filas-1,columnas)+rutas(filas,columnas-1);
}

int main(){
    int f=3,c=3;
    cout<<"Rutas posibles en una malla "<<f<<"x"<<c<<": "<<rutas(f,c)<<endl;
    return 0;
}
