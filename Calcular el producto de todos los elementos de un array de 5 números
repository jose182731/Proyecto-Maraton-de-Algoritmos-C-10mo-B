#include <iostream>
using namespace std;

int main() {
    int numeros[5], producto = 1;
    cout << "Ingrese 5 nÃºmeros:\n";

    for (int i = 0; i < 5; i++) {
        cin >> numeros[i];
        producto *= numeros[i];
    }

    cout << "El producto total es: " << producto << endl;
    return 0;
}
