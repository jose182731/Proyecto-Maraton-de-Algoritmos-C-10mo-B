#include <iostream>
using namespace std;

int main() {
    float calificacion, suma = 0;
    for (int i = 1; i <= 5; i++) {
        cout << "Ingrese calificaciÃ³n " << i << ": ";
        cin >> calificacion;
        suma += calificacion;
    }

    float promedio = suma / 5;
    cout << "El promedio es: " << promedio << endl;
    return 0;
}
