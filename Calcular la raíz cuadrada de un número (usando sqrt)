#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double num;
    cout << "Ingrese un número: ";
    cin >> num;

    if (num < 0)
        cout << "No se puede calcular la raíz cuadrada de un número negativo." << endl;
    else
        cout << "Raíz cuadrada: " << sqrt(num) << endl;

    return 0;
}
