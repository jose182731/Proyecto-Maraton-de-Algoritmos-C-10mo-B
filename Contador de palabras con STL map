#include <iostream>
#include <map>
#include <sstream>
using namespace std;

int main() {
    string texto="el sol brilla el dia es hermoso el sol calienta";
    string palabra;
    map<string,int> freq;
    stringstream ss(texto);
    while(ss>>palabra) freq[palabra]++;

    for(auto &p:freq) cout<<p.first<<": "<<p.second<<endl;
    return 0;
}
