#include <iostream>
#include <string>
using namespace std;

int main() {
    string frase;
    int cantidad = 0;

    cout << "Ingrese una frase: ";
    getline(cin, frase);

    for (char c : frase) {
        if (isdigit(c)) cantidad++;
    }

    cout << "Hay " << cantidad << " nÃºmeros en la frase." << endl;
    return 0;
}
