#include <iostream>
#include <map>
using namespace std;

int main(){
    string texto="programacion avanzada en cplusplus";
    map<char,int> frecuencia;
    for(char c : texto) if(c!=' ') frecuencia[c]++;
    for(auto &p : frecuencia)
        cout << p.first << ": " << p.second << endl;
    return 0;
}
