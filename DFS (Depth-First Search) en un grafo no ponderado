#include <iostream>
#include <vector>
using namespace std;

void dfs(int u, vector<vector<int>>& adj, vector<bool>& visitado){
    visitado[u] = true;
    cout << u << " ";
    for(int v: adj[u]){
        if(!visitado[v]) dfs(v, adj, visitado);
    }
}

int main(){
    int n=4;
    vector<vector<int>> adj(n);
    adj[0] = {1,2};
    adj[1] = {0,2};
    adj[2] = {0,1,3};
    adj[3] = {2};
    vector<bool> visitado(n,false);
    dfs(0, adj, visitado);
    return 0;
}
