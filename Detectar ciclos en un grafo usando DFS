#include <iostream>
#include <vector>
using namespace std;

bool dfs(int v, vector<vector<int>>& g, vector<int>& vis, int padre){
    vis[v]=1;
    for(int u:g[v]){
        if(!vis[u]){
            if(dfs(u,g,vis,v)) return true;
        }else if(u!=padre) return true;
    }
    return false;
}

int main(){
    int n=4;
    vector<vector<int>> g={{1,2},{0,2},{0,1,3},{2}};
    vector<int> vis(n,0);
    cout<<(dfs(0,g,vis,-1)?"Hay ciclo":"No hay ciclo")<<endl;
    return 0;
}
