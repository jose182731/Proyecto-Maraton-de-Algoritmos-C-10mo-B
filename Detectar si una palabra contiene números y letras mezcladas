#include <iostream>
#include <string>
using namespace std;

int main() {
    string palabra;
    bool tieneLetra = false, tieneNumero = false;

    cout << "Ingrese una palabra: ";
    cin >> palabra;

    for (char c : palabra) {
        if (isalpha(c)) tieneLetra = true;
        if (isdigit(c)) tieneNumero = true;
    }

    if (tieneLetra && tieneNumero)
        cout << "Contiene letras y nÃºmeros." << endl;
    else
        cout << "No contiene ambos tipos." << endl;

    return 0;
}
