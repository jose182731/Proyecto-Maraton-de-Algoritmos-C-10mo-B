#include <iostream>
using namespace std;

int main() {
    char letra;
    cout << "Ingrese una letra: ";
    cin >> letra;

    letra = tolower(letra);
    if (letra == 'a' || letra == 'e' || letra == 'i' || letra == 'o' || letra == 'u')
        cout << "Es vocal." << endl;
    else if (isalpha(letra))
        cout << "Es consonante." << endl;
    else
        cout << "No es una letra vÃ¡lida." << endl;

    return 0;
}
