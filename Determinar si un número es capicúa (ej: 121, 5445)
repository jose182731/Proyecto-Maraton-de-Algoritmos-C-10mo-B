#include <iostream>
using namespace std;

int invertir(int n) {
    int rev = 0;
    while (n > 0) {
        rev = rev * 10 + n % 10;
        n /= 10;
    }
    return rev;
}

int main() {
    int num;
    cout << "Ingrese un número: ";
    cin >> num;

    if (num == invertir(num))
        cout << "Es capicúa." << endl;
    else
        cout << "No es capicúa." << endl;

    return 0;
}
