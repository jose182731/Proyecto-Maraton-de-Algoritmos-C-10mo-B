#include <iostream>
using namespace std;

int main() {
    int num;
    cout << "Ingrese un número: ";
    cin >> num;

    if (num % 2 == 0 || num % 3 == 0)
        cout << "El número es divisible entre 2 o 3." << endl;
    else
        cout << "El número no es divisible entre 2 ni 3." << endl;

    return 0;
}
