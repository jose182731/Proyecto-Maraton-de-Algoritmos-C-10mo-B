#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;

int main() {
    const string caracteres = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789";
    string clave = "";
    srand(time(0));

    for (int i = 0; i < 8; i++) {
        clave += caracteres[rand() % caracteres.length()];
    }

    cout << "Clave generada: " << clave << endl;
    return 0;
}
