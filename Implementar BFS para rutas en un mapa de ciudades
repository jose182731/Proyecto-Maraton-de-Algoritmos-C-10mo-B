#include <iostream>
#include <vector>
#include <queue>
using namespace std;

void BFS(int inicio, vector<vector<int>>& g){
    vector<bool> visit(g.size(), false);
    queue<int> q;
    visit[inicio]=true; q.push(inicio);

    while(!q.empty()){
        int v=q.front(); q.pop();
        cout<<v<<" ";
        for(int u:g[v]){
            if(!visit[u]){ visit[u]=true; q.push(u); }
        }
    }
}

int main(){
    vector<vector<int>> ciudades={{1,2},{0,3},{0,3,4},{1,2,4},{2,3}};
    cout<<"Rutas desde ciudad 0: ";
    BFS(0,ciudades);
    return 0;
}
