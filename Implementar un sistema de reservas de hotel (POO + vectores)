#include <iostream>
#include <vector>
using namespace std;

class Habitacion {
    int numero;
    bool ocupada;
public:
    Habitacion(int n):numero(n),ocupada(false){}
    bool reservar(){
        if(!ocupada){ ocupada=true; return true; }
        return false;
    }
    void liberar(){ ocupada=false; }
    void mostrar(){ cout<<"HabitaciÃ³n "<<numero<<" -> "<<(ocupada?"Ocupada":"Libre")<<endl; }
};

int main(){
    vector<Habitacion> hotel;
    for(int i=1;i<=5;i++) hotel.emplace_back(i);
    hotel[2].reservar();
    for(auto &h:hotel) h.mostrar();
    return 0;
}
