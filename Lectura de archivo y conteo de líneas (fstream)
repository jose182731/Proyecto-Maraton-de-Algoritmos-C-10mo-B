#include <iostream>
#include <fstream>
#include <string>
using namespace std;

int main(){
    ifstream archivo("texto.txt");
    if(!archivo){
        cout << "No se pudo abrir el archivo.\n";
        return 1;
    }
    string linea;
    int contador=0;
    while(getline(archivo,linea)) contador++;
    cout << "El archivo tiene " << contador << " lÃ­neas.\n";
    archivo.close();
    return 0;
}
