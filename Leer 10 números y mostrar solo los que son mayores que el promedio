#include <iostream>
using namespace std;

int main() {
    int numeros[10];
    int suma = 0;

    cout << "Ingrese 10 números:\n";
    for (int i = 0; i < 10; i++) {
        cin >> numeros[i];
        suma += numeros[i];
    }

    float promedio = suma / 10.0;

    cout << "Números mayores al promedio (" << promedio << "): ";
    for (int i = 0; i < 10; i++) {
        if (numeros[i] > promedio)
            cout << numeros[i] << " ";
    }
    cout << endl;

    return 0;
}
