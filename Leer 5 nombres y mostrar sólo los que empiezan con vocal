#include <iostream>
#include <string>
using namespace std;

bool empiezaConVocal(string nombre) {
    char c = tolower(nombre[0]);
    return c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u';
}

int main() {
    string nombres[5];
    cout << "Ingrese 5 nombres:\n";
    for (int i = 0; i < 5; i++) {
        cin >> nombres[i];
    }

    cout << "Nombres que empiezan con vocal:\n";
    for (int i = 0; i < 5; i++) {
        if (empiezaConVocal(nombres[i])) {
            cout << nombres[i] << endl;
        }
    }

    return 0;
}
