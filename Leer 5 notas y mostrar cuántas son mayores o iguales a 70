#include <iostream>
using namespace std;

int main() {
    float nota;
    int aprobadas = 0;

    for (int i = 0; i < 5; i++) {
        cout << "Ingrese una nota: ";
        cin >> nota;
        if (nota >= 70)
            aprobadas++;
    }

    cout << "Notas aprobadas: " << aprobadas << endl;
    return 0;
}
