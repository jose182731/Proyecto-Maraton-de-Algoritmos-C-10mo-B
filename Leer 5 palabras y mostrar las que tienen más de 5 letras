#include <iostream>
#include <string>
using namespace std;

int main() {
    string palabra;
    cout << "Ingrese 5 palabras:\n";
    for (int i = 0; i < 5; i++) {
        cin >> palabra;
        if (palabra.length() > 5)
            cout << palabra << " tiene mÃ¡s de 5 letras.\n";
    }

    return 0;
}
