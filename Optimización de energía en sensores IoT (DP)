#include <iostream>
#include <vector>
using namespace std;

int energiaMaxima(vector<int>& consumo, vector<int>& beneficio, int energiaTotal){
    int n=consumo.size();
    vector<vector<int>> dp(n+1,vector<int>(energiaTotal+1,0));
    for(int i=1;i<=n;i++)
        for(int j=0;j<=energiaTotal;j++)
            dp[i][j] = (consumo[i-1]<=j) ? max(dp[i-1][j], beneficio[i-1]+dp[i-1][j-consumo[i-1]]) : dp[i-1][j];
    return dp[n][energiaTotal];
}

int main(){
    vector<int> consumo = {2,3,4};
    vector<int> beneficio = {3,4,5};
    int energiaTotal = 5;
    cout<<"Beneficio máximo con energía limitada: "<<energiaMaxima(consumo,beneficio,energiaTotal)<<endl;
    return 0;
}
