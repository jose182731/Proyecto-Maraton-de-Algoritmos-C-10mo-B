#include <bits/stdc++.h>
using namespace std;
int main(){
    int n=6;
    vector<vector<int>> g={{1,2},{3},{3},{4},{5},{}};
    vector<int> indeg(n,0);
    for(int u=0;u<n;u++) for(int v:g[u]) indeg[v]++;
    queue<int> q; for(int i=0;i<n;i++) if(indeg[i]==0) q.push(i);
    vector<int> topo;
    while(!q.empty()){
        int u=q.front(); q.pop(); topo.push_back(u);
        for(int v:g[u]) if(--indeg[v]==0) q.push(v);
    }
    if(topo.size()!=n) cout<<"Hay ciclo, no se puede topologizar\n";
    else { cout<<"Orden topolÃ³gico:\n"; for(int x:topo) cout<<x<<" "; cout<<"\n"; }
    return 0;
}
