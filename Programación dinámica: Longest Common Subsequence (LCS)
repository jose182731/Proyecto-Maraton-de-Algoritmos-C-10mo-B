#include <iostream>
#include <vector>
#include <string>
using namespace std;

int LCS(string a,string b){
    int n=a.size(), m=b.size();
    vector<vector<int>> dp(n+1,vector<int>(m+1,0));
    for(int i=1;i<=n;i++)
        for(int j=1;j<=m;j++)
            dp[i][j]= (a[i-1]==b[j-1]) ? dp[i-1][j-1]+1 : max(dp[i-1][j],dp[i][j-1]);
    return dp[n][m];
}

int main(){
    string a="AGGTAB", b="GXTXAYB";
    cout<<"Longitud de LCS: "<<LCS(a,b)<<endl;
    return 0;
}
