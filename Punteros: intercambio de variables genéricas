#include <iostream>
using namespace std;

template<typename T>
void intercambiar(T* a, T* b) {
    T temp = *a;
    *a = *b;
    *b = temp;
}

int main() {
    int x = 10, y = 20;
    intercambiar(&x, &y);
    cout << "x=" << x << ", y=" << y << endl;

    double a = 2.5, b = 8.9;
    intercambiar(&a, &b);
    cout << "a=" << a << ", b=" << b << endl;
    return 0;
}
