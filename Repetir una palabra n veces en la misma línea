#include <iostream>
#include <string>
using namespace std;

int main() {
    string palabra;
    int veces;
    cout << "Ingrese una palabra: ";
    cin >> palabra;
    cout << "¿Cuántas veces repetir?: ";
    cin >> veces;

    for (int i = 0; i < veces; i++) {
        cout << palabra << " ";
    }
    cout << endl;

    return 0;
}
