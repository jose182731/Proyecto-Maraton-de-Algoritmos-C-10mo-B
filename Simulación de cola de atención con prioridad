#include <iostream>
#include <queue>
using namespace std;

struct Cliente {
    string nombre; int prioridad;
    bool operator<(const Cliente &c) const { return prioridad < c.prioridad; }
};

int main() {
    priority_queue<Cliente> cola;
    cola.push({"Ana",2});
    cola.push({"Luis",3});
    cola.push({"Marta",1});

    while(!cola.empty()){
        auto c=cola.top(); cola.pop();
        cout<<"Atendiendo "<<c.nombre<<" (Prioridad "<<c.prioridad<<")\n";
    }
    return 0;
}
