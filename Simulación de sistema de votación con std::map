#include <iostream>
#include <map>
using namespace std;

int main(){
    map<string,int> votos;
    votos["Ana"]++;
    votos["Luis"]++;
    votos["Ana"]++;
    votos["Carlos"]++;

    cout<<"Resultados de votaciÃ³n:\n";
    for(auto &v:votos)
        cout<<v.first<<": "<<v.second<<" votos\n";
    return 0;
}
