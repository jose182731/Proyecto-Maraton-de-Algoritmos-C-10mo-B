#include <bits/stdc++.h>
using namespace std;
struct Car{int id; int time;};
int main(){
    queue<Car> cola;
    for(int i=1;i<=5;i++) cola.push({i, i*2});
    int t=0;
    while(!cola.empty()){
        Car c=cola.front(); cola.pop();
        cout<<"Auto "<<c.id<<" pasa por el semáforo en t="<<t<<"\n";
        t += c.time;
    }
    cout<<"Simulación completada en "<<t<<"s\n";
    return 0;
}
