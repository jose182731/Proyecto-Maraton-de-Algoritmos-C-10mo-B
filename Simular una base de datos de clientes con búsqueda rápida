#include <iostream>
#include <unordered_map>
using namespace std;

int main(){
    unordered_map<int,string> clientes;
    clientes[101]="Carlos"; clientes[102]="Ana"; clientes[103]="Laura";

    int id;
    cout<<"Ingrese ID de cliente: "; cin>>id;
    if(clientes.count(id))
        cout<<"Cliente encontrado: "<<clientes[id]<<endl;
    else
        cout<<"Cliente no existe.\n";
    return 0;
}
