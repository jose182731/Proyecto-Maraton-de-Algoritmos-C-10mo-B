#include <iostream>
#include <queue>
using namespace std;

struct Vuelo { string pasajero; int prioridad; bool operator<(const Vuelo &v) const { return prioridad<v.prioridad; } };

int main(){
    priority_queue<Vuelo> cola;
    cola.push({"Ana",2});
    cola.push({"Luis",3});
    cola.push({"Marta",1});

    while(!cola.empty()){
        auto v=cola.top(); cola.pop();
        cout<<"Pasajero "<<v.pasajero<<" (Prioridad "<<v.prioridad<<")\n";
    }
    return 0;
}
