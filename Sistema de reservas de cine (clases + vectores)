#include <iostream>
#include <vector>
using namespace std;

class Asiento {
    int fila, columna;
    bool ocupado;
public:
    Asiento(int f,int c):fila(f),columna(c),ocupado(false){}
    void reservar(){ ocupado=true; }
    void mostrar(){ cout<<"Asiento "<<fila<<columna<<" -> "<<(ocupado?"Ocupado":"Libre")<<endl; }
};

int main(){
    vector<Asiento> sala;
    for(int i=1;i<=3;i++)
        for(int j=1;j<=3;j++)
            sala.push_back(Asiento(i,j));

    sala[0].reservar();
    for(auto &a:sala) a.mostrar();
    return 0;
}
