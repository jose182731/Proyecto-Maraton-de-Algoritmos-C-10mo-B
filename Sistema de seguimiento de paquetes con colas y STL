#include <iostream>
#include <queue>
#include <string>
using namespace std;

int main(){
    queue<string> paquetes;
    paquetes.push("Enviado");
    paquetes.push("En tr√°nsito");
    paquetes.push("Entregado");

    while(!paquetes.empty()){
        cout<<"Estado del paquete: "<<paquetes.front()<<endl;
        paquetes.pop();
    }
    return 0;
}
