#include <iostream>
#include <vector>
using namespace std;

bool subsetSum(vector<int>& arr,int sum){
    int n=arr.size();
    vector<vector<bool>> dp(n+1,vector<bool>(sum+1,false));
    for(int i=0;i<=n;i++) dp[i][0]=true;
    for(int i=1;i<=n;i++)
        for(int j=1;j<=sum;j++)
            dp[i][j]=dp[i-1][j] || (j>=arr[i-1] && dp[i-1][j-arr[i-1]]);
    return dp[n][sum];
}

int main(){
    vector<int> arr={3,34,4,12,5,2};
    int sum=9;
    cout<<"¿Existe subconjunto con suma "<<sum<<"? "<<(subsetSum(arr,sum)?"Sí":"No")<<endl;
    return 0;
}
