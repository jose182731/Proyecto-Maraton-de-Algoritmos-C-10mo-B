#include <iostream>
#include <string>
using namespace std;

int main() {
    string password;
    cout << "Ingrese una contraseña: ";
    cin >> password;

    bool tieneNumero = false;
    for (char c : password) {
        if (isdigit(c)) {
            tieneNumero = true;
            break;
        }
    }

    if (password.length() >= 8 && tieneNumero)
        cout << "Contraseña válida." << endl;
    else
        cout << "Contraseña inválida (mín. 8 caracteres y al menos un número)." << endl;

    return 0;
}
