#include <iostream>
using namespace std;

int main() {
    char c;
    cout << "Ingrese un carácter: ";
    cin >> c;

    if (isalpha(c)) {
        if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u' ||
            c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U')
            cout << "Es una vocal." << endl;
        else
            cout << "Es una consonante." << endl;
    } else if (isdigit(c)) {
        cout << "Es un número." << endl;
    } else {
        cout << "Es un símbolo." << endl;
    }

    return 0;
}
