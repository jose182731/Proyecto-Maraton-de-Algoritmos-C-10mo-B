#include <iostream>
using namespace std;

int main() {
    int num, digitos[10] = {0};
    cout << "Ingrese un número: ";
    cin >> num;

    while (num > 0) {
        int digito = num % 10;
        if (digitos[digito]) {
            cout << "Tiene dígitos repetidos." << endl;
            return 0;
        }
        digitos[digito] = 1;
        num /= 10;
    }

    cout << "No tiene dígitos repetidos." << endl;
    return 0;
}
